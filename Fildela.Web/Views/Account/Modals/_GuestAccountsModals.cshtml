<!-- Modal account, guestaccounts, create -->
<div class="modal" id="modal-account-guestaccounts-create" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only"></span></button>
                <h4 class="modal-title">@Resource.Create_guest_account</h4>
            </div>
            <div class="modal-body">
                <div class="col-xs-12 modal-form-message-container" id="modal-account-guestaccounts-create-success-container" style="display:none;">
                    <div class="alert alert-success">
                    </div>
                </div>

                <div class="col-xs-12 modal-form-message-container" id="modal-account-guestaccounts-create-error-container" style="display:none;">
                    <div class="alert alert-danger">
                    </div>
                </div>

                <div class="col-xs-12" style="padding-top:15px;">
                    <form id="modal-account-guestaccounts-create-form" class="form-horizontal" data-error="@Resource.An_unexpected_error_has_occurred_Please_try_again">
                        @Html.AntiForgeryToken()

                        <div class="form-group form-group-container">
                            <input type="text" name="Email" class="form-control input-lg modal-account-guestaccounts-create-input" placeholder="@Resource.Email_address">
                        </div>

                        <div class="form-group form-group-container ">
                            <input type="text" name="ConfirmEmail" class="form-control input-lg modal-account-guestaccounts-create-input" placeholder="@Resource.Confirm_email_address">
                        </div>
                        <br />

                        <div class="form-group form-group-container form-group-date-picker">
                            <div class='input-group date' id='account-guestaccount-create-startdate'>
                                <input readonly type='text' name="DateStart" placeholder="@Resource.Start_date" class="form-control modal-account-guestaccounts-create-input" />
                                <span class="input-group-addon input-group-addon-datepicker">
                                    <span class="fa fa-calendar"></span>
                                </span>
                            </div>
                        </div>

                        <div class="form-group form-group-container form-group-date-picker">
                            <div class='input-group date' id='account-guestaccount-create-enddate'>
                                <input readonly type='text' name="DateExpires" placeholder="@Resource.End_date" class="form-control modal-account-guestaccounts-create-input" />
                                <span class="input-group-addon input-group-addon-datepicker">
                                    <span class="fa fa-calendar"></span>
                                </span>
                            </div>
                        </div>

                        <div class="form-group form-group-container">
                            <input type="hidden" id="account-guestaccounts-create-permissions" class="modal-account-guestaccounts-create-input" data-placeholder="@Resource.Select_rights" style="width: 100%;">
                            <input id="account-guestaccounts-create-permissions-json" name="Permissions" type="hidden" />
                        </div>
                        <br />

                        <input type="hidden" id="account-guestaccounts-create-currenttime" name="CurrentTime" />

                        <div class="form-group form-group-container">
                            <button type="submit" class="btn btn-primary btn-lg btn-block modal-account-guestaccounts-create-input modal-btn-spin" id="modal-account-guestaccounts-create-submitbtn">
                                <span>@Resource.Create</span>
                                <span id="modal-account-guestaccounts-create-submit-spin" class="spinner-modal" style="display:none;"></span>
                            </button>
                        </div>
                    </form>
                </div>
            </div>
            <div class="modal-footer" style="border: 0px;">
            </div>
        </div>
    </div>
</div>

